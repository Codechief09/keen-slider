{
  "name": "keen-slider",
  "version": "2.1.2",
  "description": "The HTML touch slider carousel with the most native feeling you will get.",
  "main": "dist/keen-slider.min.js",
  "jsnext:main": "dist/keen-slider.esm.js",
  "module": "dist/keen-slider.esm.js",
  "types": "./src/keen-slider.d.ts",
  "scripts": {
    "build": "npm run build:libs && npm run build:styles",
    "build:libs": "rollup -c",
    "build:styles": "sass src/keen-slider.scss:dist/keen-slider.min.css --style compressed && sass src/keen-slider.scss:dist/keen-slider.css",
    "build:pages": "webpack --config ./webpack.config.pages.js",
    "dev": "webpack-dev-server --config ./webpack.config.js --host 0.0.0.0 --disableHostCheck true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcbyr/keen-slider.git"
  },
  "keywords": [
    "slider",
    "carousel",
    "caroussel",
    "react",
    "vue",
    "vuejs",
    "javascript",
    "typescript",
    "gallery",
    "plugin",
    "ios"
  ],
  "author": "Eric Beyer <contact@ericbeyer.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rcbyr/keen-slider/issues"
  },
  "homepage": "https://rcbyr.github.io/keen-slider/",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
