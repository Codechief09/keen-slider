{
  "name": "keen-slider",
  "version": "6.0.1",
  "description": "The HTML touch slider carousel with the most native feeling you will get.",
  "main": "keen-slider.cjs.js",
  "jsnext:main": "keen-slider.es.js",
  "module": "keen-slider.es.js",
  "types": "keen-slider.d.ts",
  "files": [
    "keen-slider.*",
    "react.*",
    "vue.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcbyr/keen-slider.git"
  },
  "keywords": [
    "slider",
    "carousel",
    "caroussel",
    "slideshow",
    "react",
    "vue",
    "vue3",
    "vuejs",
    "javascript",
    "typescript",
    "angular",
    "webcomponents",
    "gallery",
    "plugin",
    "ios"
  ],
  "author": "Eric Beyer <contact@ericbeyer.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rcbyr/keen-slider/issues"
  },
  "homepage": "https://keen-slider.io",
  "scripts": {
    "build": "tsc && rollup -c && yarn build:types",
    "dev": "tsc-watch",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "build:types": "yarn extract && yarn fix:types",
    "extract": "yarn extract:default && yarn extract:react && yarn extract:vue",
    "extract:default": "npx api-extractor run -c ./api-extractor.json && cp -f ./keen-slider.d.ts ./keen-slider.es.d.ts",
    "extract:react": "npx api-extractor run -c ./api-extractor.react.json && cp -f ./react.d.ts ./react.es.d.ts",
    "extract:vue": "npx api-extractor run -c ./api-extractor.vue.json && cp -f ./vue.d.ts ./vue.es.d.ts",
    "fix:types": "eslint . --ext .d.ts --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@microsoft/api-extractor": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/ms": "^0.7.30",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "autoprefixer": "^10.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.1.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "rollup": "^2.6.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sass": "^1.38.1",
    "ts-jest": "^27.0.5",
    "tsc-watch": "^4.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vue": "^3.2.16"
  }
}
